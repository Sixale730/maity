{
  "cli": {
    "version": ">= 7.6.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": { "EXPO_USE_LOCAL_WORKSPACES": "1" },
      "android": { "buildType": "apk" }
    },
    "preview": {
      "distribution": "internal",
      "env": { "EXPO_USE_LOCAL_WORKSPACES": "1" },
      "android": { "buildType": "apk" }
    },
    "production": {
      "autoIncrement": true,
      "env": { "EXPO_USE_LOCAL_WORKSPACES": "1" }
    }
  },
  "submit": { "production": {} }
}
