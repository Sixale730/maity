{
  "expo": {
    "name": "Maity",
    "slug": "maity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "mx.com.maity.app",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Necesitamos acceso al micrófono para las sesiones de práctica con IA",
        "NSCameraUsageDescription": "Necesitamos acceso a la cámara para funciones de video (opcional)"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "mx.com.maity.app",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "maity",
    "plugins": [
      "expo-secure-store",
      "expo-web-browser",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "deb4e04b-1996-4e48-b85c-a555a76b7452"
      }
    }
  }
}
