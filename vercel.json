{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/accept-invite", "destination": "/api/accept-invite" },
    { "source": "/finalize-invite", "destination": "/api/finalize-invite" },
    { "source": "/(.*)", "destination": "/" }
  ]
}
